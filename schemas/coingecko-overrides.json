{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1
      },
      "logo": {
        "type": "string",
        "minLength": 1,
        "pattern": "^\\.\\/.*"
      }
    },
    "required": ["id", "logo"],
    "additionalProperties": false
  }
}
